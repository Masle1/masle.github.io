#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules/valaxy/bin/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules/valaxy/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules/valaxy/bin/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules/valaxy/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/valaxy@0.26.10_@babel+parser@7.28.5_@nuxt+kit@3.16.0_@types+markdown-it@14.1.2_@types+node@22_vaoxdbustfrzff7sioaz2vuwde/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
else
  exec node  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
fi
