#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules/vite-ssg/bin/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules/vite-ssg/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules/vite-ssg/bin/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules/vite-ssg/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/vite-ssg@28.2.2_beasties@0.3.5_unhead@2.0.19_vite@7.3.0_@types+node@22.13.10_jiti@2.6.1_sass@_nzcj7ftux4bajxjhlz63eshori/node_modules:/mnt/d/博客文件/masle.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
fi
